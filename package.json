{
  "name": "penpotqa",
  "version": "1.0.0",
  "description": "QA Test for Penpot",
  "main": "index.js",
  "scripts": {
    "open": " npx playwright test --project=chrome --ui",
    "test": "npx playwright test --project=chrome --grep-invert  'PERF'",
    "changed": "npx playwright test --project=chrome --only-changed",
    "testLogin": "npx playwright test --project=chrome --grep-invert  'PERF' ./tests/panels-features/panels-features-fill.spec.js",
    "updateSnapshots": "npx playwright test --project=chrome --project=firefox --update-snapshots",
    "firefox": "npx playwright test --project=firefox --grep-invert  'PERF' ",
    "webkit": "npx playwright test --project=webkit --grep-invert  'PERF'",
    "performance": "npx playwright test --retries=1 --timeout=555550000 --project=chrome -g 'PERF'",
    "performance:debug": "npx playwright test --debug --timeout=555550000 --project=chrome -g 'PERF'",
    "prettier": "npx prettier --write .",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/penpot/penpotqa.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/penpot/penpotqa/issues"
  },
  "homepage": "https://github.com/penpot/penpotqa#readme",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "dotenv": "^17.2.3",
    "husky": "^9.1.7",
    "playwright-dashboard-reporter": "^1.0.3"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "fs": "^0.0.1-security",
    "googleapis": "^166.0.0",
    "playwright-qase-reporter": "^2.1.6",
    "prettier": "^3.7.1",
    "qaseio": "^2.4.3",
    "stripe": "^20.0.0",
    "ts-node": "^10.9.2"
  }
}
